{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "cloudformation:*",
        "s3:*",
        "iam:PassRole",
        "iam:GetRole",
        "iam:CreateRole",
        "iam:DeleteRole",
        "iam:PutRolePolicy",
        "iam:DeleteRolePolicy",
        "iam:GetRolePolicy",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:CreatePolicy",
        "iam:GetPolicy",
        "iam:ListPolicyVersions",
        "iam:DeletePolicy",
        "iam:TagRole",
        "iam:TagPolicy",
        "ecr:*",
        "ec2:*",
        "elasticache:*",
        "ssm:*", 
        "cloudwatch:*",
        "logs:*",
        "kms:Describe*",
        "kms:List*",
        "kms:Create*"
      ],
      "Resource": "*"
    }
  ]
}
